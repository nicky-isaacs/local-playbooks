[alias]
  b = branch
  ru = remote update
  co = checkout
  cob = checkout -b
  st = status --short --ignore-submodules
  dc = diff --cached
  d = diff
  amend = commit --amend
  a = add
  aa = add --all
  ai = add -i
  cp = cherry-pick -n
[push]
  # Only push branches that have been set up to track a remote branch.
  default = current
